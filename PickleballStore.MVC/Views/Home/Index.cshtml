<!-- /header -->
<!-- slider -->
<div class="tf-slideshow slider-effect-fade slider-skincare position-relative">
    <div dir="ltr" class="swiper tf-sw-slideshow" data-preview="1" data-tablet="1" data-mobile="1"
         data-centered="false" data-space="0" data-loop="false" data-auto-play="true" data-delay="1000"
         data-speed="1000">
        <div class="swiper-wrapper">
            @foreach (var slider in Model.Sliders)
            {
                <div class="swiper-slide" lazy="true">
                    <div class="wrap-slider">
                        <img class="lazyload"
                             data-src="@Url.Content("~/images/slider/" + slider.ImageName)"
                             src="@Url.Content("~/images/slider/" + slider.ImageName)"
                             alt="@slider.Title"
                             loading="lazy">
                        <div class="box-content">
                            <div class="container">
                                <div class="card-box-2">
                                    @if (!string.IsNullOrEmpty(slider.SubTitle))
                                    {
                                        <div class="fade-item fade-item-1 text-extra-heading text_white fw-8">
                                            @Html.Raw(slider.SubTitle)
                                        </div>
                                    }
                                    @if (!string.IsNullOrEmpty(slider.Title))
                                    {
                                        <div class="fade-item fade-item-2 title">
                                            <a asp-controller="Product"
                                               asp-action="Details"
                                               asp-route-id="@slider.ProductId"
                                               class="link text-white">
                                                @slider.Title
                                            </a>
                                        </div>
                                    }

                                    @if (slider.NewPrice.HasValue)
                                    {
                                        <div class="fade-item fade-item-3 price">
                                            @if (slider.OldPrice.HasValue)
                                            {
                                                <span class="old-price">$@slider.OldPrice</span>
                                            }
                                            <span class="new-price text_primary">$@slider.NewPrice</span>
                                        </div>
                                    }

                                    <div class="fade-item fade-item-4">
                                        <a pickleball-black-1.jpg
                                           class="tf-btn btn-line btn-line-light collection-other-link fw-6">
                                            <span>Shop now</span>
                                            <i class="icon icon-arrow1-top-left"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <div class="wrap-pagination sw-absolute-3">
        <div class="sw-dots style-2 dots-white sw-pagination-slider justify-content-center"></div>
    </div>
</div>
<!-- /slider -->
<!-- Iconbox -->
<section class="line">
    <div class="container">
        <div class="wrap-spacing-iconbox wow fadeInUp" data-wow-delay="0s">
            <div dir="ltr" class="swiper tf-sw-recent" data-preview="3" data-tablet="3" data-mobile="1"
                 data-space-lg="30" data-space-md="30" data-space="15" data-pagination="1" data-pagination-md="1"
                 data-pagination-lg="1">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <div class="tf-icon-box-v2 justify-content-center">
                            <span class="icon icon-shipping"></span>
                            <div class="title">Free Shipping on Orders Over $120</div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="tf-icon-box-v2 justify-content-center">
                            <span class="icon icon-trial"></span>
                            <div class="title">100-days risk-free trial</div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="tf-icon-box-v2 justify-content-center">
                            <span class="icon icon-warranty"></span>
                            <div class="title">Free 4-years warranty</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="sw-dots style-2 sw-pagination-recent justify-content-center"></div>
        </div>
    </div>
</section>
<!-- /Iconbox -->
<!-- Categories -->
<section class="flat-spacing-3 flat-categorie">
    <div class="container">
        <div class="flat-title-v2">
            <div class="box-sw-navigation">
                <div class="nav-sw nav-next-slider nav-next-collection">
                    <span class="icon icon-arrow-left"></span>
                </div>
                <div class="nav-sw nav-prev-slider nav-prev-collection">
                    <span class="icon icon-arrow-right"></span>
                </div>
            </div>
            <span class="text-3 fw-7 text-uppercase title wow fadeInUp" data-wow-delay="0s">
                SHOP BY CATEGORIES
            </span>
        </div>
        <div class="row">
            <div class="col-xl-9 col-lg-8 col-md-8">
                <div dir="ltr" class="swiper tf-sw-collection" data-preview="3" data-tablet="2" data-mobile="2"
                     data-space-lg="30" data-space-md="30" data-space="15" data-loop="false"
                     data-auto-play="false">
                    <div class="swiper-wrapper">
                        @foreach (var category in Model.Categories.Where(c => !c.IsDeleted))
                        {
                            <div class="swiper-slide" lazy="true">
                                <div class="collection-item style-left hover-img">
                                    <div class="collection-inner">
                                        <a asp-controller="Shop" asp-action="ShopByCategory" asp-route-id="@category.Id"
                                           class="collection-image img-style">
                                            <img class="lazyload"
                                                 data-src="@Url.Content($"~/images/collections/{category.ImageName}")"
                                                 src="@Url.Content($"~/images/collections/{category.ImageName}")"
                                                 alt="@category.Name">
                                        </a>
                                        <div class="collection-content left-0">
                                            <a asp-controller="Shop" asp-action="ShopByCategory" asp-route-id="@category.Id"
                                               class="tf-btn collection-title hover-icon fs-15">
                                                <span>@category.Name</span><i class="icon icon-arrow1-top-left"></i>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-lg-4 col-md-4">
                <div class="discovery-new-item">
                    <h5>Discovery all new items</h5>
                    <a asp-controller="Shop" asp-action="Index"><i class="icon-arrow1-top-left"></i></a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- /Categories -->
<!-- Product -->
<section class="flat-spacing-3">
    <div class="flat-title">
        <span class="title wow fadeInUp" data-wow-delay="0s">Pickleball Paddle Tiers</span>
        <p class="sub-title fs-18 text_black-2 wow fadeInUp" data-wow-delay="0s">
            Choose from our range of premium paddles that offer different levels of control and power.
        </p>
    </div>
    <div class="tf-grid-layout tf-col-2 md-col-3 gap-0">
        @foreach (var product in Model.Products)
        {
            <div class="card-product type-line-padding @(product.Variants.Count <= 1 ? "none-hover" : "")">
                <div class="card-product-wrapper">
                    <a href="@Url.Action("Details", "Product", new { id = product.Id })" class="product-img">
                        @{
                            var mainImage = product.Images.FirstOrDefault(x => x.IsMain) ?? product.Images.FirstOrDefault();
                            var hoverImage = product.Images.FirstOrDefault(x => x.IsHover);
                        }
                        
                        @if (mainImage != null)
                        {
                            <img class="lazyload img-product" 
                                 data-src="images/products/@mainImage.ImageName"
                                 src="~/images/products/@mainImage.ImageName" 
                                 alt="@product.Name">
                        }
                        
                        @if (hoverImage != null)
                        {
                            <img class="lazyload img-hover" 
                                 data-src="images/products/@hoverImage.ImageName"
                                 src="~/images/products/@hoverImage.ImageName" 
                                 alt="@product.Name">
                        }
                    </a>
                    
                    <div class="list-product-btn absolute-2">
                        <a href="#" data-bs-toggle="modal" data-bs-target="#quick_add_@product.Id"
                           class="box-icon bg_white quick-add tf-btn-loading quick-add-btn">
                            <span class="icon icon-bag"></span>
                            <span class="tooltip">Quick Add</span>
                        </a>
                        <a href="#" data-bs-toggle="modal" data-bs-target="#quick_view_@product.Id"
                           class="box-icon bg_white quickview tf-btn-loading quick-view-btn">
                            <span class="icon icon-view"></span>
                            <span class="tooltip">Quick View</span>
                        </a>
                    </div>
                    
                    @if (!string.IsNullOrEmpty(product.BadgeLabel))
                    {
                        <div class="on-sale-wrap text-end">
                            <div class="on-sale-item @product.BadgeCssClass">@product.BadgeLabel</div>
                        </div>
                    }
                    <div class="card-product-wrapper position-relative">
                        <a href="@Url.Action("Details", "Product", new { id = product.Id })" class="product-img">
                        </a>

                        @if (product.CountdownEndDate.HasValue && product.CountdownEndDate.Value > DateTime.Now)
                        {
                                <div class="js-countdown"
                                     data-countdown="@product.CountdownEndDate.Value.ToString("o")"
                                     data-labels="Days,Hours,Minutes,Seconds"
                                     data-visible-labels="d,h,m,s">
                                </div>
                        }
                    </div>
                </div>
                
                <div class="card-product-info text-center">
                    <a href="@Url.Action("Details", "Product", new { id = product.Id })" class="title link">
                        @product.Name
                    </a>
                    <div class="price">
                        @if (product.PriceBeforeDiscount.HasValue && product.PriceBeforeDiscount.Value > product.Price)
                        {
                            <span class="old-price">$@product.PriceBeforeDiscount.Value.ToString("F2")</span>
                            <span class="new-price">$@product.Price.ToString("F2")</span>
                        }
                        else
                        {
                            <span class="price">$@product.Price.ToString("F2")</span>
                        }
                    </div>                 
                    @if (product.Variants.Count > 1)
                    {
                        <ul class="list-color-product justify-content-center">
                            @foreach (var variant in product.Variants.Take(5))
                            {
                                <li class="list-color-item color-swatch @(product.Variants.IndexOf(variant) == 0 ? "active" : "")"
                                    data-product-id="@product.Id" 
                                    data-variant-color="@variant.ColorCode"
                                    data-variant-image="@variant.OptionImageName">
                                    <span class="tooltip">@variant.OptionValue</span>
                                    <span class="swatch-value" style="background-color: @variant.ColorCode"></span>
                                    @if (!string.IsNullOrEmpty(variant.OptionImageName))
                                    {
                                        <img class="lazyload" 
                                             data-src="/images/products/@variant.OptionImageName"
                                             src="~/images/products/@variant.OptionImageName" 
                                             alt="@variant.OptionValue @product.Name">
                                    }
                                </li>
                            }
                            @if (product.Variants.Count > 5)
                            {
                                <li class="list-color-item more-colors">
                                    <span class="tooltip">+@(product.Variants.Count - 5) more</span>
                                    <span class="swatch-value">+@(product.Variants.Count - 5)</span>
                                </li>
                            }
                        </ul>
                    }
                </div>
            </div>
        }
    
    </div>
</section>
<!-- /Product -->
<!-- modal quick -->
@foreach (var product in Model.Products)
{
    <partial name="_ProductQuickViewModalPartial" model="product"></partial>

    <partial name="_ProductQuickAddModalPartial" model="product"></partial>
}
<!-- /modal quick -->
